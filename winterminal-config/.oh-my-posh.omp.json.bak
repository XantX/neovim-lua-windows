{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "root",
          "style": "plain",
          "foreground": "#d79921",
          "properties": {
            "root_icon": "\uF0E7"
          }
        },
        {
          "foreground": "#ffffff",
          "properties": {
            "prefix": "",
            "text": " <#98971a>Win </><#d65d0e>❯</><#98971a>❯</>"
          },
          "style": "plain",
          "type": "text"
        },
        {
          "foreground": "#458588",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "type": "path"
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "#d79921",
          "properties": {
            "branch_icon": "",
            "display_status": true,
            "display_status_detail": false,
            "prefix": "<#458588>git:(</>",
            "postfix": "<#458588>)</>"
          }
        },
        {
          "foreground": "#b16286",
          "properties": {
            "display_exit_code": false,
            "prefix": " "
          },
          "style": "plain",
          "type": "exit"
        },
        {
          "foreground": "#8ec07c",
          "properties": {
            "time_format": "3:04"
          },
          "style": "plain",
          "type": "time"
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "#8ec07c",
          "properties": {
            "prefix": " \uE718 "
          }
        },
        {
          "type": "dotnet",
          "style": "plain",
          "foreground": "#ffffff",
          "properties": {
            "prefix": " \uE77F "
          }
        },
        {
          "type": "java",
          "style": "plain",
          "foreground": "#8ec07c",
          "properties": {
            "prefix": " \uE738 "
          }
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "#fabd2f",
          "properties": {
            "prefix": " \uE235 "
          }
        }
      ],
      "type": "prompt"
    },

    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "type": "spotify",
          "style": "plain",
          "foreground": "#8ec07c",
          "properties": {
            "prefix": "\uF9C6 ",
            "playing_icon": "\uE602 ",
            "paused_icon": "\uF8E3 ",
            "stopped_icon": "\uF04D ",
            "track_separator": " - "
          }
        }
      ]
    }
  ],
  "final_space": true 
}
